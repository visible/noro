{
  "name": "noro light",
  "type": "light",
  "colors": {
    "editor.background": "#f5f3ef",
    "editor.foreground": "#1c1917",
    "editor.lineHighlightBackground": "#ebe8e3",
    "editor.selectionBackground": "#c53d4340",
    "editorLineNumber.foreground": "#a8a29e",
    "editorLineNumber.activeForeground": "#57534e",
    "editorCursor.foreground": "#c53d43",
    "editorWhitespace.foreground": "#e7e5e4",
    "editorIndentGuide.background": "#e7e5e4",
    "editorIndentGuide.activeBackground": "#d6d3d1",
    "activityBar.background": "#ebe8e3",
    "activityBar.foreground": "#1c1917",
    "activityBar.inactiveForeground": "#78716c",
    "activityBarBadge.background": "#c53d43",
    "activityBarBadge.foreground": "#ffffff",
    "sideBar.background": "#ebe8e3",
    "sideBar.foreground": "#44403c",
    "sideBarTitle.foreground": "#1c1917",
    "sideBarSectionHeader.background": "#f5f3ef",
    "statusBar.background": "#ebe8e3",
    "statusBar.foreground": "#78716c",
    "statusBar.debuggingBackground": "#c53d43",
    "statusBar.debuggingForeground": "#ffffff",
    "titleBar.activeBackground": "#f5f3ef",
    "titleBar.activeForeground": "#1c1917",
    "titleBar.inactiveBackground": "#f5f3ef",
    "titleBar.inactiveForeground": "#78716c",
    "tab.activeBackground": "#f5f3ef",
    "tab.activeForeground": "#1c1917",
    "tab.inactiveBackground": "#ebe8e3",
    "tab.inactiveForeground": "#78716c",
    "tab.activeBorder": "#c53d43",
    "terminal.background": "#f5f3ef",
    "terminal.foreground": "#1c1917",
    "terminal.ansiBlack": "#1c1917",
    "terminal.ansiRed": "#dc2626",
    "terminal.ansiGreen": "#16a34a",
    "terminal.ansiYellow": "#ca8a04",
    "terminal.ansiBlue": "#2563eb",
    "terminal.ansiMagenta": "#9333ea",
    "terminal.ansiCyan": "#0891b2",
    "terminal.ansiWhite": "#f5f3ef",
    "terminal.ansiBrightBlack": "#57534e",
    "terminal.ansiBrightRed": "#ef4444",
    "terminal.ansiBrightGreen": "#22c55e",
    "terminal.ansiBrightYellow": "#eab308",
    "terminal.ansiBrightBlue": "#3b82f6",
    "terminal.ansiBrightMagenta": "#a855f7",
    "terminal.ansiBrightCyan": "#06b6d4",
    "terminal.ansiBrightWhite": "#fafaf9",
    "focusBorder": "#c53d43",
    "selection.background": "#c53d4340",
    "input.background": "#ffffff",
    "input.foreground": "#1c1917",
    "input.border": "#e7e5e4",
    "input.placeholderForeground": "#a8a29e",
    "button.background": "#c53d43",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#b91c1c",
    "list.activeSelectionBackground": "#c53d4320",
    "list.activeSelectionForeground": "#1c1917",
    "list.hoverBackground": "#ebe8e3"
  },
  "tokenColors": [
    { "scope": ["comment", "punctuation.definition.comment"], "settings": { "foreground": "#a8a29e", "fontStyle": "italic" } },
    { "scope": ["keyword", "storage.type", "storage.modifier"], "settings": { "foreground": "#c53d43" } },
    { "scope": ["string", "string.quoted"], "settings": { "foreground": "#57534e" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#b45309" } },
    { "scope": ["constant.language"], "settings": { "foreground": "#c53d43" } },
    { "scope": ["entity.name.function", "support.function"], "settings": { "foreground": "#1c1917" } },
    { "scope": ["entity.name.class", "entity.name.type"], "settings": { "foreground": "#b45309" } },
    { "scope": ["variable", "variable.other"], "settings": { "foreground": "#1c1917" } },
    { "scope": ["variable.parameter"], "settings": { "foreground": "#44403c" } },
    { "scope": ["punctuation"], "settings": { "foreground": "#78716c" } },
    { "scope": ["entity.name.tag"], "settings": { "foreground": "#c53d43" } },
    { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#b45309" } },
    { "scope": ["support.type.property-name"], "settings": { "foreground": "#1c1917" } },
    { "scope": ["meta.object-literal.key"], "settings": { "foreground": "#44403c" } }
  ]
}
