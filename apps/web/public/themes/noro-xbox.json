{
  "name": "noro xbox",
  "type": "dark",
  "colors": {
    "editor.background": "#0e0e0e",
    "editor.foreground": "#ffffff",
    "activityBar.background": "#0e0e0e",
    "activityBar.foreground": "#107C10",
    "sideBar.background": "#141414",
    "sideBar.foreground": "#cccccc",
    "statusBar.background": "#107C10",
    "statusBar.foreground": "#ffffff",
    "titleBar.activeBackground": "#0e0e0e",
    "titleBar.activeForeground": "#ffffff",
    "tab.activeBackground": "#1a1a1a",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveBackground": "#0e0e0e",
    "tab.inactiveForeground": "#888888",
    "editor.lineHighlightBackground": "#1a1a1a",
    "editor.selectionBackground": "#107C1050",
    "editorCursor.foreground": "#107C10",
    "editorLineNumber.foreground": "#444444",
    "editorLineNumber.activeForeground": "#107C10",
    "editorIndentGuide.background": "#1a1a1a",
    "editorIndentGuide.activeBackground": "#333333",
    "focusBorder": "#107C10",
    "input.background": "#141414",
    "input.foreground": "#ffffff",
    "input.border": "#333333",
    "button.background": "#107C10",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#0d6b0d",
    "list.activeSelectionBackground": "#107C1040",
    "list.hoverBackground": "#1a1a1a",
    "gitDecoration.addedResourceForeground": "#3CDB4E",
    "gitDecoration.modifiedResourceForeground": "#40CCD0",
    "gitDecoration.deletedResourceForeground": "#D04242",
    "terminal.foreground": "#ffffff",
    "terminal.background": "#0e0e0e",
    "terminal.ansiBlack": "#0e0e0e",
    "terminal.ansiRed": "#D04242",
    "terminal.ansiGreen": "#3CDB4E",
    "terminal.ansiYellow": "#ECDB33",
    "terminal.ansiBlue": "#40CCD0",
    "terminal.ansiMagenta": "#9b59b6",
    "terminal.ansiCyan": "#40CCD0",
    "terminal.ansiWhite": "#ffffff",
    "terminal.ansiBrightBlack": "#444444",
    "terminal.ansiBrightRed": "#e74c3c",
    "terminal.ansiBrightGreen": "#4ade80",
    "terminal.ansiBrightYellow": "#fef08a",
    "terminal.ansiBrightBlue": "#67e8f9",
    "terminal.ansiBrightMagenta": "#c084fc",
    "terminal.ansiBrightCyan": "#67e8f9",
    "terminal.ansiBrightWhite": "#ffffff"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#444444", "fontStyle": "italic" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#107C10" }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": { "foreground": "#3CDB4E" }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": { "foreground": "#ECDB33" }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#40CCD0" }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class"],
      "settings": { "foreground": "#ECDB33" }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": { "foreground": "#ffffff" }
    },
    {
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#D04242" }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#D04242" }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#3CDB4E" }
    },
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": { "foreground": "#888888" }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": { "foreground": "#40CCD0" }
    }
  ]
}
