  "tokenColors": [
    { "scope": ["comment", "punctuation.definition.comment"], "settings": { "foreground": "#57534e", "fontStyle": "italic" } },
    { "scope": ["keyword", "storage.type", "storage.modifier"], "settings": { "foreground": "#ff6b00" } },
    { "scope": ["string", "string.quoted"], "settings": { "foreground": "#a8a29e" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#fbbf24" } },
    { "scope": ["constant.language"], "settings": { "foreground": "#ff6b00" } },
    { "scope": ["entity.name.function", "support.function"], "settings": { "foreground": "#fafaf9" } },
    { "scope": ["entity.name.class", "entity.name.type"], "settings": { "foreground": "#fbbf24" } },
    { "scope": ["variable", "variable.other"], "settings": { "foreground": "#fafaf9" } },
    { "scope": ["variable.parameter"], "settings": { "foreground": "#d6d3d1" } },
    { "scope": ["punctuation"], "settings": { "foreground": "#78716c" } },
    { "scope": ["entity.name.tag"], "settings": { "foreground": "#ff6b00" } },
    { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#fbbf24" } },
    { "scope": ["support.type.property-name"], "settings": { "foreground": "#fafaf9" } },
    { "scope": ["meta.object-literal.key"], "settings": { "foreground": "#d6d3d1" } }
  ]
}
